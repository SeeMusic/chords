    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "4.3.9",
