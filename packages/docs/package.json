{
  "name": "@seemusic/docs",
  "private": false,
  "version": "0.0.1",
  "description": "看见音乐中后台组件库",
  "license": "MIT",
  "author": "xsq",
  "type": "module",
  "scripts": {
    "docs:dev": "pnpm run register:components && vitepress dev",
