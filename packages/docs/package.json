    "vitepress": "1.0.0-beta.5",
    "vitepress-theme-demoblock": "^3.0.2"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@seemusic/element-plus-theme-sop": "^2.3.6",
    "@seemusic/styles": "^0.0.1",
    "@seemusic/ui-components": "workspace:^",
    "@vueuse/core": "^10.2.1",
    "element-plus": "2.3.6",
    "vue-router": "^4.2.2"
  }
}
